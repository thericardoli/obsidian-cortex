{
    "name": "obsidian-sample-plugin",
    "version": "1.0.0",
    "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
    "type": "module",
    "main": "main.js",
    "scripts": {
        "dev": "vite build --watch",
        "build": "vite build --mode production",
        "build:dev": "vite build --mode development",
        "build:check": "tsc -noEmit -skipLibCheck && bun run build",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "svelte-check": "svelte-check --tsconfig tsconfig.json",
        "format": "prettier --write \"**/*.{ts,js,svelte,json,css,md}\"",
        "format:check": "prettier --check \"**/*.{ts,js,svelte,json,css,md}\"",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@internationalized/date": "^3.8.1",
        "@lucide/svelte": "^0.544.0",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/cli": "^4.1.17",
        "@tailwindcss/vite": "^4.1.17",
        "@types/node": "^16.11.6",
        "bits-ui": "^2.14.1",
        "clsx": "^2.1.1",
        "eslint": "^9",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-svelte": "^3",
        "globals": "^16.5.0",
        "obsidian": "latest",
        "prettier": "^3.7.4",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "svelte": "^5.45.4",
        "svelte-check": "^4.3.4",
        "svelte-eslint-parser": "^1.4.0",
        "tailwind-merge": "^3.4.0",
        "tailwind-variants": "^3.2.2",
        "tailwindcss": "^4.1.17",
        "tslib": "2.4.0",
        "tw-animate-css": "^1.4.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8",
        "vite": "^7.2.6"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^2.0.53",
        "@ai-sdk/google": "^2.0.44",
        "@ai-sdk/openai": "^2.0.77",
        "@openai/agents": "^0.3.4",
        "@openai/agents-extensions": "^0.3.4",
        "@openrouter/ai-sdk-provider": "^1.3.0",
        "ai": "^5.0.106",
        "runed": "^0.37.0",
        "svelte-streamdown": "^3.0.0",
        "zod": "3"
    }
}
